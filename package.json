{
  "name": "shadow-logger",
  "version": "0.7.0",
  "description": "Debugger dashboard for express apps",
  "bin": "./lib/cli.js",
  "main": "./lib/main.js",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "server": "node ./lib/cli.js up prod",
    "server:dev": "node ./lib/cli.js up dev",
    "watch": "tsc --watch -p tsconfig.jsonc",
    "start": "cd ./dashboard && react-scripts start",
    "start:localhost": "cd ./dashboard && REACT_APP_API_URL=http://localhost/logs react-scripts start",
    "build:dev": "cd ./dashboard && REACT_APP_API_URL=http://localhost:8081/logs PUBLIC_URL=/debug react-scripts build",
    "build": "cd ./dashboard && react-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beplatform/express-debugger.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beplatform/express-debugger/issues"
  },
  "homepage": "https://github.com/beplatform/express-debugger#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.142.0",
    "@aws-sdk/types": "^3.127.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/express": "^4.17.13",
    "aws-sdk": "^2.1107.0",
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^5.0.0",
    "express": "^4.17.2",
    "mocha": "^9.2.0",
    "moment": "^2.29.1",
    "pino-abstract-transport": "^1.0.0",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "ra-data-json-server": "^3.19.7",
    "react": "^17.0.2",
    "react-admin": "^3.19.7",
    "react-admin-json-view": "^1.2.1",
    "react-dom": "^17.0.2",
    "react-scripts": "^5",
    "web-vitals": "^2.1.0"
  }
}
